{
    "name": "drone-biotech-webapp",
    "version": "0.0.0",
    "scripts": {
        "prettier:check": "prettier --check 'src/**/*.{ts,js,json,css,scss,html}'",
        "prettier:fix": "prettier --write 'src/**/*.{ts,js,json,css,scss,html}'",
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "jest",
        "test:ci": "jest --runInBand",
        "test:coverage": "jest --coverage",
        "test:update": "jest --updateSnapshot",
        "prepare": "husky install",
        "lint": "ng lint",
        "lint:fix": "eslint --fix 'src/**/*.{ts,js}'",
        "stylelint": "stylelint 'src/**/*.{css,scss}'",
        "stylelint:fix": "stylelint --fix 'src/**/*.{css,scss}'"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^18.1.0",
        "@angular/cdk": "^18.2.10",
        "@angular/common": "^18.1.2",
        "@angular/compiler": "^18.1.2",
        "@angular/core": "^18.1.2",
        "@angular/fire": "^18.0.1",
        "@angular/forms": "^18.1.2",
        "@angular/material": "^18.2.10",
        "@angular/platform-browser": "^18.1.2",
        "@angular/platform-browser-dynamic": "^18.1.2",
        "@angular/router": "^18.1.2",
        "@jsverse/transloco": "^7.5.0",
        "@jsverse/transloco-locale": "^7.0.1",
        "@jsverse/transloco-messageformat": "^7.0.1",
        "@jsverse/transloco-persist-lang": "^7.0.1",
        "@jsverse/transloco-persist-translations": "^7.0.1",
        "@ngrx/effects": "^18.1.1",
        "@ngrx/router-store": "^18.1.1",
        "@ngrx/store": "^18.1.1",
        "@ngrx/store-devtools": "^18.1.1",
        "firebase": "^10.14.1",
        "flyonui": "^1.1.0",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^18.1.1",
        "@angular/cli": "^18.1.1",
        "@angular/compiler-cli": "^18.1.0",
        "@eslint/js": "^9.8.0",
        "@jest/globals": "^29.7.0",
        "@types/jest": "^29.5.13",
        "@types/swiper": "^5.4.3",
        "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.38",
        "@typescript-eslint/parser": "^8.0.0-alpha.38",
        "angular-eslint": "18.1.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^9.8.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.8.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-preset-angular": "^14.2.4",
        "postcss": "^8.4.40",
        "prettier": "^3.3.3",
        "prettier-eslint": "^16.3.0",
        "stylelint": "^16.8.1",
        "stylelint-config-standard": "^36.0.1",
        "swiper": "^11.1.14",
        "tailwindcss": "^3.4.7",
        "typescript": "~5.5.2",
        "typescript-eslint": "^8.0.0-alpha.38"
    },
    "lint-staged": {
        "*.{js,ts}": "eslint --fix",
        "*.{css,scss}": "stylelint --fix",
        "*.{html,json}": "prettier . --write"
    }
}
