  "devDependencies": {
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.7",
    "@eslint/js": "^9.8.0",
    "@jest/globals": "^29.7.0",
    "@tailwindcss/postcss": "^4.1.12",
